# .bashrc

# Source global definitions
if [ -f /etc/bashrc ]; then
        . /etc/bashrc
fi

# User specific aliases and functions
export ZENHOME=/opt/zenoss
export PYTHONPATH=$ZENHOME/lib/python
# DP - Moving zenhome bin forward, as I got totally messed by picking up python2.6 on ubuntu
# since this is isolated to only the zenoss user, I think this is safe enough
export PATH=$ZENHOME/bin:$PATH:<%= @java_bin %>
export INSTANCE_HOME=$ZENHOME
